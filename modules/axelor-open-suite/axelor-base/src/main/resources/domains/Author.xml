<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  https://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

    <module name="base" package="com.axelor.apps.base.db"/>

    <entity name="Author">
        <string name="firstName" required="true" />
        <string name="lastName" required="true" />
        <string name="login" required="true" unique="true"/>
        <many-to-one name="event" ref="com.axelor.apps.base.db.EventList"  />
    </entity>
</domain-models>